      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{041CE30C-F43E-4107-81C3-3F0A5C6B1446}"
          SQLTask:SqlStatementSource="IF OBJECT_ID('Dim_Fecha', 'U') IS NOT NULL&#xA;BEGIN&#xA;    DELETE FROM Dim_Fecha;&#xA;END;&#xA;&#xA;DECLARE @FechaInicio DATE = '2015-01-01';&#xA;DECLARE @FechaFin    DATE = '2018-12-31';&#xA;&#xA;;WITH CteFechas AS (&#xA;    SELECT @FechaInicio AS Fecha&#xA;    UNION ALL&#xA;    SELECT DATEADD(DAY, 1, Fecha)&#xA;    FROM CteFechas&#xA;    WHERE Fecha &lt; @FechaFin&#xA;)&#xA;INSERT INTO Dim_Fecha&#xA;(&#xA;    FechaKey,&#xA;    FechaCompleta,&#xA;    Anio,&#xA;    Mes,&#xA;    Dia,&#xA;    NombreMes,&#xA;    NombreDiaSemana,&#xA;    Trimestre,&#xA;    EsFinDeSemana&#xA;)&#xA;SELECT&#xA;    CONVERT(INT, FORMAT(Fecha, 'yyyyMMdd'))     AS FechaKey,&#xA;    Fecha                                       AS FechaCompleta,&#xA;    YEAR(Fecha)                                 AS Anio,&#xA;    MONTH(Fecha)                                AS Mes,&#xA;    DAY(Fecha)                                  AS Dia,&#xA;    DATENAME(MONTH, Fecha)                      AS NombreMes,&#xA;    DATENAME(WEEKDAY, Fecha)                    AS NombreDiaSemana,&#xA;    DATEPART(QUARTER, Fecha)                    AS Trimestre,&#xA;    CASE &#xA;        WHEN DATENAME(WEEKDAY, Fecha) IN ('Saturday','Sunday') &#xA;             THEN 1 &#xA;        ELSE 0 &#xA;    END                                         AS EsFinDeSemana&#xA;FROM CteFechas&#xA;OPTION (MAXRECURSION 32767);&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
  </DTS:Executables>
