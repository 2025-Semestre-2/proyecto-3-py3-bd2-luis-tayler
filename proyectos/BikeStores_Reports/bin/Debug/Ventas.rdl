    <DataSet Name="dsListaMarcas">
      <Query>
        <DataSourceName>dsVentasDW</DataSourceName>
        <CommandText>SELECT '' AS Marca
UNION ALL
SELECT DISTINCT Marca
FROM Dim_Productos
WHERE Marca IS NOT NULL
ORDER BY Marca;</CommandText>
      </Query>
      <Fields>
        <Field Name="Marca">
          <DataField>Marca</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
