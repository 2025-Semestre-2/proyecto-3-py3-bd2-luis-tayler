  <DataSets>
    <DataSet Name="ReporteVentas">
      <Query>
        <DataSourceName>dsVentasDW</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Cliente">
            <Value>=Parameters!Cliente.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Marca">
            <Value>=Parameters!Marca.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@FechaInicio">
            <Value>=Parameters!FechaInicio.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@FechaFin">
            <Value>=Parameters!FechaFin.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
    p.Categoria,
    p.NombreProducto,
    s.NombreTienda      AS Nombre_Tienda,
    f.Anio,
    SUM(v.CantidadVendida)    AS Cantidad_Vendida,
    SUM(v.MontoBrutoLinea)    AS MontoTotal,
    SUM(v.MontoNetoLinea)     AS Monto_Neto_Linea,
    SUM(v.CantidadFacturas)   AS Cantidad_Facturas
FROM Fact_Ventas v
JOIN Dim_Productos  p ON v.ProductoKey   = p.ProductoKey
JOIN Dim_Clientes   c ON v.ClienteKey    = c.ClienteKey
JOIN Dim_Sucursales s ON v.SucursalKey   = s.SucursalKey
JOIN Dim_Fecha      f ON v.FechaOrdenKey = f.FechaKey
WHERE
    (@Cliente     IS NULL OR c.NombreCompleto = @Cliente)
    AND (@Marca   IS NULL OR p.Marca         = @Marca)
    AND (@FechaInicio IS NULL OR f.FechaCompleta &gt;= @FechaInicio)
    AND (@FechaFin    IS NULL OR f.FechaCompleta &lt;= @FechaFin)
GROUP BY
    p.Categoria,
    p.NombreProducto,
    s.NombreTienda,
    f.Anio
ORDER BY
    p.Categoria,
    p.NombreProducto,
    s.NombreTienda,
    f.Anio;
</CommandText>
      </Query>
      <Fields>
        <Field Name="Categoria">
          <DataField>Categoria</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="NombreProducto">
          <DataField>NombreProducto</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Nombre_Tienda">
          <DataField>Nombre_Tienda</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Anio">
          <DataField>Anio</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Cantidad_Vendida">
          <DataField>Cantidad_Vendida</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="MontoTotal">
          <DataField>MontoTotal</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Monto_Neto_Linea">
          <DataField>Monto_Neto_Linea</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Cantidad_Facturas">
          <DataField>Cantidad_Facturas</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsListaClientes">
      <Query>
        <DataSourceName>dsVentasDW</DataSourceName>
        <CommandText>SELECT '' AS NombreCompleto
UNION ALL
SELECT DISTINCT NombreCompleto FROM Dim_Clientes
</CommandText>
      </Query>
      <Fields>
